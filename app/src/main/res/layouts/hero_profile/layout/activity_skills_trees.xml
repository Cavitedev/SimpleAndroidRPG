<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/skillsTrees_frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".application.party.partyEdition.heroProfile.skills.SkillsTreesActivity">

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/skillsTrees_viewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/skillsTrees_bottomNavigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/colorPrimaryDark"
        app:menu="@menu/skills_trees_menu" />

</FrameLayout>